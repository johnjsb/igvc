<?xml version='1.0'?>
<robot name="hal" xmlns:body="http://playerstage.sourceforge.net/gazebo/xmlschema/#body" xmlns:controller="http://playerstage.sourceforge.net/gazebo/xmlschema/#controller" xmlns:geom="http://playerstage.sourceforge.net/gazebo/xmlschema/#geom" xmlns:sensor="http://playerstage.sourceforge.net/gazebo/xmlschema/#sensor" xmlns:xacro="http://ros.org/wiki/xacro">
    <link name='chassis'>
      <inertial>
        <mass value="0.001" />
        <origin xyz="0 0 0" />
        <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001" />
      </inertial>
      
      <collision name='base1_c'>
        <origin xyz="-0.2413 0 0.29685" rpy="0 0 0"/>
        <geometry>
          <box size="0.6858 0.4318 0.3937"/>
        </geometry>
      </collision>

      <visual name='base1_v'>
        <origin xyz="-0.2413 0 0.29685" rpy="0 0 0"/>
        <geometry>
          <box size="0.6858 0.4318 0.3937"/>
        </geometry>
      </visual>

      <collision name='base2_c'>
        <origin xyz="-0.2413 0 0.6588" rpy="0 0 0"/>
        <geometry>
          <box size="0.6858 0.6096 0.3302"/>
        </geometry>
      </collision>

      <visual name='base2_v'>
        <origin xyz="-0.2413 0 0.6588" rpy="0 0 0"/>
        <geometry>
          <box size="0.6858 0.6096 0.3302"/>
        </geometry>
      </visual>

      <collision name='base3_c'>
        <origin xyz="0.0889 0 1.10965" rpy="0 0 0"/>
        <geometry>
          <box size="0.4826 0.6096 0.5715"/>
        </geometry>
      </collision>

      <visual name='base3_v'>
        <origin xyz="0.0889 0 1.10965" rpy="0 0 0"/>
        <geometry>
          <box size="0.4826 0.6096 0.5715"/>
        </geometry>
      </visual>
    </link>
    
    <link name="left_caster">
      <inertial>
        <mass value="30"/>
        <origin xyz="0 0 0"/>
        <inertia ixx="5" ixy="0" ixz="0" iyy="5" iyz="0" izz="5"/>
      </inertial>
      
      <collision name='left_caster_c'>
        <origin xyz="-0.4318 0.2286 0.1" rpy="0 0 0"/>
        <geometry>
          <sphere radius="0.1"/>
        </geometry>
      </collision>
  
      <visual name='left_caster_v'>
        <origin xyz="-0.4318 0.2286 0.1" rpy="0 0 0"/>
        <geometry>
          <sphere radius="0.1"/>
        </geometry>
      </visual>
    </link>
    
    <link name="right_caster">
      <inertial>
        <mass value="30"/>
        <origin xyz="0 0 0"/>
        <inertia ixx="5" ixy="0" ixz="0" iyy="5" iyz="0" izz="5"/>
      </inertial>
      
      <collision name='right_caster_c'>
        <origin xyz="-0.4318 -0.2286 0.1" rpy="0 0 0"/>
        <geometry>
          <sphere radius="0.1"/>
        </geometry>
      </collision>
  
      <visual name='right_caster_v'>
        <origin xyz="-0.4318 -0.2286 0.1" rpy="0 0 0"/>
        <geometry>
          <sphere radius="0.1"/>
        </geometry>
      </visual>
    </link>
    
    <joint name="left_caster_joint" type="fixed">
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <parent link="chassis"/>
      <child link="left_caster"/>
    </joint>
    
    <joint name="right_caster_joint" type="fixed">
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <parent link="chassis"/>
      <child link="right_caster"/>
    </joint>
      
    <link name="left_wheel">
      <inertial>
        <mass value="30"/>
        <origin xyz="0 0 0"/>
        <inertia ixx="5" ixy="0" ixz="0" iyy="5" iyz="0" izz="5"/>
      </inertial>
      
      <collision name="collision">
        <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
        <geometry>
          <cylinder radius="0.127" length="0.0762"/>
        </geometry>
      </collision>
      
      <visual name="visual">
        <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
        <geometry>
          <cylinder radius="0.127" length="0.0762"/>
        </geometry>
      </visual>
    </link>
    
    <link name="right_wheel">
      <inertial>
        <mass value="30"/>
        <origin xyz="0 0 0"/>
        <inertia ixx="5" ixy="0" ixz="0" iyy="5" iyz="0" izz="5"/>
      </inertial>
    
      <collision name="collision">
        <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
        <geometry>
          <cylinder radius="0.127" length="0.0762"/>
        </geometry>
      </collision>
      
      <visual name="visual">
        <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
        <geometry>
          <cylinder radius="0.127" length="0.0762"/>
        </geometry>
      </visual>
    </link>
    
    <joint type="revolute" name="left_wheel_hinge">
      <origin xyz="0.0 0.254 0.127" rpy="0 0 0"/>
      <child link="left_wheel"/>
      <parent link="chassis"/>
      <axis xyz="0 1 0"/>
      <dynamics damping="0" friction="0" />
      <limit effort="3000" velocity="3000" lower="-1e+20" upper="1e+20" />
      <safety_controller k_position="100" k_velocity="100" soft_lower_limit="-3000" soft_upper_limit="3000" />
    </joint>
    <transmission name="right_wheel_trans" type="pr2_mechanism_model/SimpleTransmission">
      <actuator name="right_wheel_motor"/>
      <joint name="left_wheel_hinge"/>
      <mechanicalReduction>1.0</mechanicalReduction>
    </transmission>
    
    <joint type="revolute" name="right_wheel_hinge">
      <origin xyz="0.0 -0.254 0.127" rpy="0 0 0"/>
      <child link="right_wheel"/>
      <parent link="chassis"/>
      <axis xyz="0 1 0"/>
      <dynamics damping="0" friction="0" />
      <limit effort="3000" velocity="3000" lower="-1e+20" upper="1e+20" />
      <safety_controller k_position="100" k_velocity="100" soft_lower_limit="-3000" soft_upper_limit="3000" />
    </joint>
    <transmission name="right_wheel_trans" type="pr2_mechanism_model/SimpleTransmission">
      <actuator name="right_wheel_motor"/>
      <joint name="right_wheel_hinge"/>
      <mechanicalReduction>1.0</mechanicalReduction>
    </transmission>
 
    <link name="left_camera_link">
      <inertial>
        <mass value="0.0001"/>
        <origin xyz="0 0 0"/>
        <inertia ixx="0.000001" ixy="0" ixz="0" iyy="0.000001" iyz="0" izz="0.000001"/>
      </inertial>
      <collision name="left_camera_c">
        <origin xyz="0.3302 0.05 1.3854" rpy="0 1.047 0"/>
        <geometry>
          <box size="0.05 0.05 0.05"/>
        </geometry>
      </collision>
      <visual name="visual">
        <origin xyz="0.3302 0.05 1.3854" rpy="0 1.047 0"/>
        <geometry>
          <box size="0.05 0.05 0.05"/>
        </geometry>
      </visual>
    </link>
    
    <joint name="left_camera_joint" type="fixed">
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <parent link="chassis"/>
      <child link="left_camera_link"/>
    </joint>
 
    <link name="right_camera_link">
      <inertial>
        <mass value="0.0001"/>
        <origin xyz="0 0 0"/>
        <inertia ixx="0.000001" ixy="0" ixz="0" iyy="0.000001" iyz="0" izz="0.000001"/>
      </inertial>
      <collision name="right_camera_c">
        <origin xyz="0.3302 -0.05 1.3854" rpy="0 1.047 0"/>
        <geometry>
          <box size="0.05 0.05 0.05"/>
        </geometry>
      </collision>
      <visual name="visual">
        <origin xyz="0.3302 -0.05 1.3854" rpy="0 1.047 0"/>
        <geometry>
          <box size="0.05 0.05 0.05"/>
        </geometry>
      </visual>
    </link>
    
    <joint name="right_camera_joint" type="fixed">
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <parent link="chassis"/>
      <child link="right_camera_link"/>
    </joint>
    
    <gazebo reference="left_camera_link">
      <sensor type="camera" name="left_camera">
        <update_rate>30.0</update_rate>
        <camera name="left_camera">
          <horizontal_fov>0.995</horizontal_fov>
          <image>
            <width>640</width>
            <height>480</height>
            <format>R8G8B8</format>
          </image>
          <clip>
            <near>0.02</near>
            <far>100</far>
          </clip>
        </camera>
        <plugin name="camera_controller" filename="libgazebo_ros_camera.so">
          <alwaysOn>true</alwaysOn>
          <updateRate>0.0</updateRate>
          <cameraName>left_camera</cameraName>
          <imageTopicName>image_raw</imageTopicName>
          <cameraInfoTopicName>camera_info</cameraInfoTopicName>
          <frameName>left_camera_link</frameName>
          <hackBaseline>0.07</hackBaseline>
          <distortionK1>0.0</distortionK1>
          <distortionK2>0.0</distortionK2>
          <distortionK3>0.0</distortionK3>
          <distortionT1>0.0</distortionT1>
          <distortionT2>0.0</distortionT2>
        </plugin>
      </sensor>
      <material>Gazebo/Black</material>
    </gazebo> 
      
      
    <gazebo reference="right_camera_link">
      <sensor type="camera" name="right_camera">
        <update_rate>30.0</update_rate>
